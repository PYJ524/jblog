<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="post">

	<!-- 쿼리문 작성 -->
	<insert id = "insert" parameterType="com.javaex.vo.PostVo">
		<![CDATA[
			insert into 
			post values(
			    seq_post_no.nextval,
			    #{cateNo},
			    #{postTitle},
			    #{postContent},
			    sysdate
			)
		]]>
	</insert>
</mapper>
